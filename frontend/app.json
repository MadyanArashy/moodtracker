{
  "expo": {
    "name": "MoodTracker",
    "slug": "moodtracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/moodtracker-logo.png",
    "scheme": "moodtracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.madyanarashy.moodtracker",
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/moodtracker-logo-transparent.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/moodtracker-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "694d309a-30cb-46ae-88f7-b72d70903e61"
      }
    },
    "owner": "madyan-arashy"
  }
}
